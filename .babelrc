{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions", "Firefox ESR", "> 1%", "ie >= 10", "iOS >= 8", "Android >= 4"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "helpers": true
        // "regenerator": true,
        // "useESModules": false
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
